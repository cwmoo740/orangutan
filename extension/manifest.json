{
  "manifest_version": 2,
  "version": "1.0",
  "name": "Orangutan",
  "description": "Replace mentions of Donald Trump with references to an orangutan",
  "author": "cwmoo740",
  "permissions": [
    "activeTab",
    "storage"
  ],
  "browser_action": {
    "default_title": "Orangutan"
  },
  "background": {
    "scripts": ["event.js"],
    "persistent": false
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "orangutan.js"
      ],
      "run_at": "document_end"
    }
  ]
}